msgstr ""

# ../lib/gamedata/class.txt:323
#: ../lib/gamedata/class.txt:323
msgid "Teleports you randomly within the current level."
msgstr ""

# ../lib/gamedata/class.txt:330
#: ../lib/gamedata/class.txt:330
