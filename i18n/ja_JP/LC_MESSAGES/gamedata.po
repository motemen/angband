# #J0#
#: ../lib/gamedata/monster.txt:6120
msgctxt "monster"
msgid "Ulwarth, Son of Ulfang"
msgstr "ウルファングの息子『ウルワルス』"

# From 変愚蛮怒 3.0.0Alpha68
# #J0#
# #J0#
# #J0#(コメント)
