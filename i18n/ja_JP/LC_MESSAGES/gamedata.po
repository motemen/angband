msgstr ""

#: ../lib/gamedata/class.txt:1723
msgctxt "class"
msgid "Restores STR, INT, CON, and experience at the cost of 66 HP."
msgstr ""

#: ../lib/gamedata/class.txt:1729
msgctxt "class"
msgid ""
