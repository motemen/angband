msgid "It is a strangely moving puddle."
msgstr "それは奇妙に動く泥のようなものだ。"

#: ../lib/gamedata/monster.txt:5565
msgctxt "monster"
msgid "Easterling champion"
msgstr ""

#: ../lib/gamedata/monster.txt:5589
msgctxt "monster"
msgid "A scarred warrior who wields an axe and moves with confidence."
msgstr ""

#: ../lib/gamedata/monster.txt:5590
msgctxt "monster"
msgid "Azog, Enemy of the Dwarves"
msgstr ""

#: ../lib/gamedata/monster.txt:5617
msgctxt "monster"
