#: ../src/list-mon-message.h:12
msgctxt "list-mon-message"
msgid "[is|are] hurt."
msgstr "は傷ついた。"

#: ../src/list-mon-message.h:13
msgctxt "list-mon-message"
msgid "die[s]."
msgstr "は死んだ。"

#: ../src/list-mon-message.h:14
msgctxt "list-mon-message"
msgid "[is|are] destroyed."
msgstr "は破壊された。"

#: ../src/list-mon-message.h:15
msgctxt "list-mon-message"
msgid "resist[s] a lot."
msgstr "にはかなり耐性がある。"

#: ../src/list-mon-message.h:16
msgctxt "list-mon-message"
msgid "[is|are] hit hard."
msgstr "はひどい痛手を受けた。"

#: ../src/list-mon-message.h:17
msgctxt "list-mon-message"
msgid "resist[s]."
msgstr "には耐性がある。"

#: ../src/list-mon-message.h:18
msgctxt "list-mon-message"
msgid "[is|are] immune."
msgstr "には完全な耐性がある。"

#: ../src/list-mon-message.h:19
msgctxt "list-mon-message"
msgid "resist[s] somewhat."
msgstr "はいくらか耐性がある。"

#: ../src/list-mon-message.h:20
msgctxt "list-mon-message"
msgid "[is|are] unaffected!"
msgstr "には影響がなかった！"

#: ../src/list-mon-message.h:21
msgctxt "list-mon-message"
msgid "spawn[s]!"
msgstr "が分裂した！"

#: ../src/list-mon-message.h:22
msgctxt "list-mon-message"
msgid "look[s] healthier."
msgstr "は元気になったようだ。"

#: ../src/list-mon-message.h:23
msgctxt "list-mon-message"
msgid "fall[s] asleep!"
msgstr "は眠ってしまった！"

#: ../src/list-mon-message.h:24
msgctxt "list-mon-message"
msgid "wake[s] up."
msgstr "は目を覚ました。"

#: ../src/list-mon-message.h:25
msgctxt "list-mon-message"
msgid "cringe[s] from the light!"
msgstr "は光に身をすくめた！"

#: ../src/list-mon-message.h:26
msgctxt "list-mon-message"
msgid "shrivel[s] away in the light!"
msgstr "は光を受けてしぼんでしまった！"

#: ../src/list-mon-message.h:27
msgctxt "list-mon-message"
msgid "lose[s] some skin!"
msgstr "の表皮が失われた！"

#: ../src/list-mon-message.h:28
msgctxt "list-mon-message"
msgid "dissolve[s]!"
msgstr "は溶けてしまった！"

#: ../src/list-mon-message.h:29
msgctxt "list-mon-message"
msgid "catch[es] fire!"
msgstr "に火がついた！"

#: ../src/list-mon-message.h:30
msgctxt "list-mon-message"
msgid "[is|are] badly frozen."
msgstr "は凍ってしまった。"

#: ../src/list-mon-message.h:31
msgctxt "list-mon-message"
msgid "shudder[s]."
msgstr "は身震いした。"

#: ../src/list-mon-message.h:32
msgctxt "list-mon-message"
msgid "change[s]!"
msgstr "が変身した！"

#: ../src/list-mon-message.h:33
msgctxt "list-mon-message"
msgid "disappear[s]!"
msgstr "の姿が消えた！"

#: ../src/list-mon-message.h:34
msgctxt "list-mon-message"
msgid "[is|are] even more stunned."
msgstr "はさらに朦朧としている。"

#: ../src/list-mon-message.h:35
msgctxt "list-mon-message"
msgid "[is|are] stunned."
msgstr "は朦朧としている。"

#: ../src/list-mon-message.h:36
msgctxt "list-mon-message"
msgid "[is|are] no longer stunned."
msgstr "は朦朧状態から立ち直った。"

#: ../src/list-mon-message.h:37
msgctxt "list-mon-message"
msgid "look[s] more confused."
msgstr "はさらに混乱したようだ。"

#: ../src/list-mon-message.h:38
msgctxt "list-mon-message"
msgid "look[s] confused."
msgstr "は混乱したようだ。"

#: ../src/list-mon-message.h:39
msgctxt "list-mon-message"
msgid "[is|are] no longer confused."
msgstr "はもう混乱していない。"

#: ../src/list-mon-message.h:40
msgctxt "list-mon-message"
msgid "look[s] more slowed."
msgstr "はさらに遅くなったようだ。"

#: ../src/list-mon-message.h:41
msgctxt "list-mon-message"
msgid "look[s] slowed."
msgstr "の動きが遅くなった。"

#: ../src/list-mon-message.h:42
msgctxt "list-mon-message"
msgid "speed[s] up."
msgstr "の速さが元に戻った。"

#: ../src/list-mon-message.h:43
msgctxt "list-mon-message"
msgid "look[s] even faster!"
msgstr "はさらに速くなったようだ！"

#: ../src/list-mon-message.h:44
msgctxt "list-mon-message"
msgid "start[s] moving faster."
msgstr "の動きが速くなった。"

#: ../src/list-mon-message.h:45
msgctxt "list-mon-message"
msgid "slow[s] down."
msgstr "の動きが遅くなった。"

#: ../src/list-mon-message.h:46
msgctxt "list-mon-message"
msgid "look[s] more terrified!"
msgstr "はさらに恐怖したようだ！"

#: ../src/list-mon-message.h:47
msgctxt "list-mon-message"
msgid "flee[s] in terror!"
msgstr "は恐怖に逃げ出した！"

#: ../src/list-mon-message.h:48
msgctxt "list-mon-message"
msgid "[is|are] no longer afraid."
msgstr "は恐怖から立ち直った。"

#: ../src/list-mon-message.h:49
msgctxt "list-mon-message"
msgid "[is|are] frozen to the spot!"
msgstr "は氷漬けにされた！"

#: ../src/list-mon-message.h:50
msgctxt "list-mon-message"
msgid "can move again."
msgstr "は動けるようになった。"

#: ../src/list-mon-message.h:51
msgctxt "list-mon-message"
msgid "seem[s] less magical!"
msgstr "の魔法の力が弱まったようだ！"

#: ../src/list-mon-message.h:52
msgctxt "list-mon-message"
msgid "seem[s] magical again."
msgstr "は魔法の力を取り戻した。"

#: ../src/list-mon-message.h:53
msgctxt "list-mon-message"
msgid "falls under your spell!"
msgstr "はあなたの言葉に従い始めた！"

#: ../src/list-mon-message.h:54
msgctxt "list-mon-message"
msgid "is no longer under your control."
msgstr "はもう操られていない。"

#: ../src/list-mon-message.h:55
msgctxt "list-mon-message"
msgid "shimmers for a moment."
msgstr "は一瞬ゆらめいた。"

#: ../src/list-mon-message.h:56
msgctxt "list-mon-message"
msgid "You hear [a|several] scream[|s] of agony!"
msgstr "苦痛の叫び声が聞こえてきた！"

#: ../src/list-mon-message.h:57
msgctxt "list-mon-message"
msgid "disintegrate[s]!"
msgstr "は崩壊した！"

#: ../src/list-mon-message.h:58
msgctxt "list-mon-message"
msgid "freeze[s] and shatter[s]!"
msgstr "は凍ってバラバラになった！"

#: ../src/list-mon-message.h:59
msgctxt "list-mon-message"
msgid "lose[s] some mana!"
msgstr "はマナを失った！"

#: ../src/list-mon-message.h:60
msgctxt "list-mon-message"
msgid "look[s] briefly puzzled."
msgstr "は少し困っているようだ。"

#: ../src/list-mon-message.h:61
msgctxt "list-mon-message"
msgid "maintain[s] the same shape."
msgstr "は姿を保っている。"

#: ../src/list-mon-message.h:62
msgctxt "list-mon-message"
msgid "[is|are] unharmed."
msgstr "は無傷だ。"

#: ../src/list-mon-message.h:63
msgctxt "list-mon-message"
msgid "appear[s]!"
msgstr "が現れた！"

#: ../src/list-mon-message.h:64
msgctxt "list-mon-message"
msgid "There is a puff of smoke!"
msgstr "煙が吹き出した！"
